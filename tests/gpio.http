@BASE_URL=http://127.0.0.1:8899

### init gpio out
POST {{BASE_URL}}/fetch
content-type: application/json

{
    "message": {
        "gpioOutputInit": {
            "pin": 25,
            "value": true
        }
    }
}

### set false value 
POST {{BASE_URL}}/fetch
content-type: application/json

{
    "message": {
        "gpioOutputSet": {
            "pin": 25,
            "state": false
        }
    }
}


### set true value 
POST {{BASE_URL}}/fetch
content-type: application/json

{
    "message": {
        "gpioOutputSet": {
            "pin": 25,
            "state": true
        }
    }
}